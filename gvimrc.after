" color github
color railscasts

" Set Font for GVIM/MVIM
let s:uname = system("uname")

if s:uname == "Darwin\n"
  " OSX configuration
  ":set guifont=Consolas:h16
  :set guifont=Monaco:h18
endif

if s:uname == "Linux\n"
  " Obviously Linux configuration
  :set guifont=Monospace
endif

set transparency=5
